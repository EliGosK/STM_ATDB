  <system.web>
    <globalization uiCulture="en-US" culture="en-US" />
    <customErrors mode="On" defaultRedirect="/Error/Index?statusCode=500">
      <error statusCode="401" redirect="/Error/Index?statusCode=401" />
      <error statusCode="403" redirect="/Error/Index?statusCode=403" />
      <error statusCode="404" redirect="/Error/Index?statusCode=404" />
    </customErrors>
    <compilation debug="true" targetFramework="4.5" />
    <httpRuntime targetFramework="4.5" maxRequestLength="30000" />
    <pages controlRenderingCompatibilityVersion="4.0">
      <namespaces>
        <add namespace="MvcSiteMapProvider.Web.Html" />
        <add namespace="MvcSiteMapProvider.Web.Html.Models" />
      </namespaces>
    </pages>
    <siteMap defaultProvider="MvcSiteMapProvider">
      <providers>
        <clear />
        <add name="MvcSiteMapProvider" type="MvcSiteMapProvider.DefaultSiteMapProvider, MvcSiteMapProvider" siteMapFile="~/Mvc.Sitemap" />
      </providers>
    </siteMap>
    <authentication mode="Forms">
      <forms defaultUrl="~/" loginUrl="~/Account/LogIn"></forms>
    </authentication>
  </system.web>
